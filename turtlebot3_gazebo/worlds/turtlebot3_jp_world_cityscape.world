          </waypoint>
          <waypoint>
            <time>25</time>
            <pose>-9 1.1 0 0 0 3.14</pose>
          </waypoint>
          <waypoint>
            <time>27.5</time>
            <pose>-9 6.5 0 0 0 0</pose>
          </waypoint>
          <waypoint>
            <time>30</time>
            <pose>9.5 6.5 0 0 0 0</pose>
          </waypoint>
          <waypoint>
            <time>32.5</time>
            <pose>9.5 1.1 0 0 0 -3.14</pose>
          </waypoint>
          <waypoint>
            <time>37.5</time>
